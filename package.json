{
  "name": "anthonyjcastro-dot-com",
  "version": "2.0.0",
  "private": true,
  "homepage": "https://www.anthonyjcastro.com/",
  "scripts": {
    "start": "webpack serve --env environment=development --mode development",
    "build": "webpack --env environment=production  --mode production",
    "lint": "eslint .",
    "lint:ci": "eslint . --quiet",
    "prettier": "prettier . --write",
    "prettier:ci": "prettier . --check",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf build/*",
    "build:cy": "webpack --env environment=cypress --mode production",
    "serve:cy": "serve -s build",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "@analytics/google-analytics": "1.0.7",
    "analytics": "0.8.11",
    "modern-normalize": "2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.1.8"
  },
  "devDependencies": {
    "@anthony-j-castro/eslint-config": "github:anthony-j-castro/eslint-config#semver:1.3.0",
    "@anthony-j-castro/prettier-config": "github:anthony-j-castro/prettier-config#semver:1.0.0",
    "@babel/core": "7.24.0",
    "@babel/preset-env": "7.24.0",
    "@types/react": "18.2.60",
    "@types/react-dom": "18.2.19",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "6.10.0",
    "cypress": "13.6.6",
    "dotenv-webpack": "8.0.1",
    "eslint": "8.57.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.0",
    "prettier": "3.2.5",
    "serve": "14.2.1",
    "style-loader": "3.3.4",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
